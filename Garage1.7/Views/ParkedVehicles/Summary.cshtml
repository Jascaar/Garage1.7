@model IEnumerable<Garage1._7.Models.SummaryViewModel>

@{
    ViewBag.Title = "Summary of parked vehicles";
}

<h2>Summary of parked vehicles</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Count)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SumTires)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ParkingTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccumulatedRevenue)
        </th>
     </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>

                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Count)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SumTires)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ParkingTime) Minutes
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AccumulatedRevenue) SEK
            </td>
        </tr>
    }

</table>

<div class="form-actions no-color hidden-print">

    @Html.ActionLink("Back to List", "Index")
</div>

